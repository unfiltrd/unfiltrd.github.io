---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>unfiltrd</title>
	<link href="http://unfiltrd.github.io/directory/atom.xml" rel="self"/>
	<link href="http://unfiltrd.github.io/directory"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://unfiltrd.github.io/directory</id>
	<author>
		<name>Sophia Cordoba</name>
		<name>Derek Peng</name>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://unfiltrd.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://unfiltrd.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>